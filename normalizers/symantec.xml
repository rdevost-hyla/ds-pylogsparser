<!DOCTYPE normalizer SYSTEM "normalizer.dtd">
<normalizer name="symantec"
            version="0.99"
            unicode="yes"
            ignorecase="yes"
            matchtype="match"
            appliedTo="raw"
            taxonomy="antivirus">
 <description>
  <localized_desc language="en">This normalizer parses messages issued by the Symantec Antivirus.</localized_desc>
  <localized_desc language="fr">Ce normaliseur analyse les messages Ã©mis par Symantec Antivirus.</localized_desc>
 </description>
 <authors>
  <author>fbo@wallix.com</author>
 </authors>
 <tagTypes>
  <tagType name="SymantecDate" type="str">
   <description>
    <localized_desc language="en"></localized_desc>
    <localized_desc language="fr"></localized_desc>
   </description>
   <regexp>^(?:[A-F0-9]{2}){6}$</regexp>
  </tagType>
  <tagType name="SymantecCategory" type="str">
   <description>
    <localized_desc language="en"></localized_desc>
    <localized_desc language="fr"></localized_desc>
   </description>
   <regexp>[1-4]</regexp>
  </tagType>
  <tagType name="SymantecEvent" type="str">
   <description>
    <localized_desc language="en"></localized_desc>
    <localized_desc language="fr"></localized_desc>
   </description>
   <regexp>\d{1,3}</regexp>
  </tagType>
  <tagType name="SymantecQAS" type="str">
   <description>
    <localized_desc language="en"></localized_desc>
    <localized_desc language="fr"></localized_desc>
   </description>
   <regexp>[012]?</regexp>
  </tagType>
 </tagTypes>
 <callbacks>
  <callback name="decode_date">
    date_params = [ int(value[2*i : 2*(i+1)], 16) for i in range(6) ]
    date_params[0] += 1970
    date_params[1] += 1
    log['date'] = datetime(*date_params)
  </callback>
